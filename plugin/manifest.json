{
  "name": "cc_plugin",
  "manifest_version": 2,
  "version": "1.0",
  "description": "chrome console plugin",
  "content_scripts": [{"matches": ["*://*/*"],"js": ["content.js"]}],
  "permissions": ["tabs","nativeMessaging","activeTab","<all_urls>","webNavigation"],
  "background": {
  "scripts": ["background.js"]
}
}